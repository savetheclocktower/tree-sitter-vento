{
  "name": "vento",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "tag"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "comment_tag"
            },
            "named": true,
            "value": "tag"
          },
          {
            "type": "SYMBOL",
            "name": "content"
          }
        ]
      }
    },
    "content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "PATTERN",
        "value": "[^(\\{\\{)]+"
      }
    },
    "keyword": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9\\(\\)\\.]+"
    },
    "close_keyword": {
      "type": "PATTERN",
      "value": "\\/[a-zA-Z]+"
    },
    "codeSnippet": {
      "type": "PATTERN",
      "value": "[^\\/][\\s]+"
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "keyword"
              },
              {
                "type": "SYMBOL",
                "name": "codeSnippet"
              }
            ]
          },
          "named": true,
          "value": "code"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "close_keyword"
          },
          "named": true,
          "value": "keyword"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PREC_LEFT",
              "value": 1,
              "content": {
                "type": "SYMBOL",
                "name": "keyword"
              }
            },
            {
              "type": "SYMBOL",
              "name": "code"
            }
          ]
        }
      ]
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "{{"
            },
            {
              "type": "STRING",
              "value": "{{-"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "filter"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "}}"
            },
            {
              "type": "STRING",
              "value": "-}}"
            }
          ]
        }
      ]
    },
    "filter": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "|>"
          },
          {
            "type": "SYMBOL",
            "name": "code"
          }
        ]
      }
    },
    "comment": {
      "type": "PATTERN",
      "value": "#[^#]+#"
    },
    "comment_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "code"
    },
    {
      "type": "SYMBOL",
      "name": "keyword"
    }
  ],
  "inline": [],
  "supertypes": []
}

